<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="myCollectionBean" class="com.paypal.beans.CollectionsDI">
		<property name="myList">
			<list>
				<value>10</value>
				<value>20</value>
				<value>30</value>
			</list>
		</property>
		
		<property name="myMap">
			<map>
				<entry key="1">
					<value>One</value>
				</entry>
				<entry key="2" value="Two" />
				<entry key="3">
					<value>Three</value>
				</entry>
			</map>
		</property>
		
		<property name="mySet">
			<set>
				<value>10</value>
				<value>65</value>
				<value>5</value>
				<value>10</value>
				<value>6</value>
			</set>
		</property>
	</bean>
	
</beans>
